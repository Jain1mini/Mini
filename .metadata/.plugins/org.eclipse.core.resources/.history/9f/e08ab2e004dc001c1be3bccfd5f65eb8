import java.io.*;


public class One {

	public static void main(String[] args) {
		
		String a = "Ram";
		String b = "Sita";
		
		  String c = add(a,b);
		System.out.println("the sum of a and b is =" +c);
	}
	
	
		
	public static String add(Serializable a, String b)
	{
	Object s;
	s = a + b;
	return b;
	}
}