import java.io.*;


public class One {

	public static void main(String[] args) {
		
		 double a = 30;
		 
		 // converting values to radian
		 double b = Math.toRadians(a);
		 
		 // return the trigonometric sine of a
		 System.out.println("Sine value of a is:" +Math.sin(a));
			
		
		// return the trigonometric cosine of a
		System.out.println("Cosine value of a is:" +Math.cos(a));
		
		// return the trigonometric tangent of a
		System.out.println("Tangent value of a is:" +Math.tan(a));
		
		// return the trigonometric arc sine of a
		System.out.println("Sine value of a is:" +Math.asin(a));
		
		//return the trigonometric arc cosine of a
		System.out.println("Cosine value of a is:" +Math.acos(a));
		
		// return the trigonometric arc tangent of a
		System.out.println("Tangent value of a is:" +Math.atan(a));
		
		// return the trigonometric hyperbolic sine of a
		System.out.println("Sine value of a is:" +Math.sinh(a));
		
		// return the trigonometric hyperbolic cosine of a
		System.out.println("Sine value of a is:" +Math.cosh(a));
		
		// return the trigonometric hyperbolic tangent of a
		System.out.println("Sine value of a is:" +Math.tanh(a));
	}
}