import java.io.*;

public class One {

	public static void main(String[] args) {
	
		// creating scanner class object
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter the no:");
		
		// reading value from the user
		
		int num = scan.nextInt();
		// method calling 
		
		findEvenOdd(num);
		
