import java.io.*;


public class One {

	public static void main(String[] args) {
		
		  int a = 10;
		 int b = 20;
		
		 //method calling
		   int c = add(a,b);
		   // a and b are actual parameters
		System.out.println("the sum of a and b is =" +c);
	}
	
	
		
	public static int add(int n1, int n2)
	// n1 and n2 are formal parameters
	
	{
	int s;
	s = n1 + n2;
	return s;
	
	// returning the sum
	}
}